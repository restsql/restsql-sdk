<?xml version="1.0" encoding="UTF-8"?>
<rs:sqlResource xmlns:rs="http://restsql.org/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://restsql.org/schema SqlResource.xsd " defaultDatabase="sakila">
	<query>
		select film_id "id", title, release_year "year", language_id
		from film
	</query>
	<metadata>
		<database default="sakila" />
		<table name="film" alias="movie" role="Parent" />
	</metadata>
</rs:sqlResource>
